<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>aroberts.obsidian.commit</string>
    <key>ProgramArguments</key>
    <array>
      <string>/Users/aroberts/bin/autocommit.sh</string>
      <string>/Users/aroberts/Library/Mobile Documents/iCloud~md~obsidian/Documents/main</string>
