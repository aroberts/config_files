[alias]
  c = commit -m
  a = add
  di = diff
  dic = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
  unpushed = log HEAD..origin
  incoming = !sh -c 'git fetch && git whatchanged -p ..origin'
